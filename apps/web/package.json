{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm generate-routes && tsc -b && vite build",
    "lint": "eslint .",
    "cleanup": "del-cli src/graphql/generated --force || exit 0",
    "preview": "vite preview",
    "generate-routes": "tsr generate",
    "generate-graphql": "pnpm cleanup && graphql-codegen --config client/codegen.ts",
    "generate": "pnpm generate-routes && pnpm generate-graphql"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@clerk/clerk-react": "^5.53.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-router": "^1.133.13",
    "@tanstack/react-router-devtools": "^1.133.13",
    "@vanilla-extract/css": "^1.17.4",
    "clsx": "^2.1.1",
    "graphql": "^16.11.0",
    "lucide-react": "^0.546.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rxjs": "^7.8.1",
    "tailwindcss": "^4.1.14",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/client-preset": "^5.1.1",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@nexly/db": "workspace:*",
    "@nexly/eslint-config": "workspace:*",
    "@nexly/typescript-config": "workspace:*",
    "@nexly/ui": "workspace:*",
    "@tanstack/router-cli": "^1.133.13",
    "@tanstack/router-plugin": "^1.133.13",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.4",
    "del-cli": "^7.0.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14"
  }
